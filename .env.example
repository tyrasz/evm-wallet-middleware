# Server Configuration
PORT=3000
NODE_ENV=development
LOG_LEVEL=info

# Database
# Connect to SQLite file by default
DATABASE_URL="file:./dev.db"

# Security
# 32-byte hex string for AES-256-GCM (Example: 64 hex characters)
# Generate with: openssl rand -hex 32
MASTER_KEY=

# Blockchain
# Optional: RPC URL for mainnet/testnet (defaults to public ones if not set)
RPC_URL_SEPOLIA=https://rpc.sepolia.org
